                <div><b>Gender:</b> {{gender}} </div> 
               <div><b>Age:</b> {{age}} </div> 
               <br>
              {{/each}}
              </ul>
              </li>
               {{/each}}
               {{#if groups}}
               </ul>
               {{/if}}
               </li>
               {{/each}}
               </ul>

                {{#if groups}}
                <div><i>Groups:</i></div> 
              <ul>
              {{/if}}

               {{#each groups}}
               <li>
               <div><b>ID:</b> {{id}} </div>        
               <div><b>Name:</b> {{name}} </div> 
                <div><b>Description:</b> {{description}} </div> 
                <br>
                <ul>

                <div><i>Members:</i></div>
               {{#each members}}
                <div><b>ID:</b> {{id}} </div>        
               <div><b>Name:</b> {{name}} </div> 
                <div><b>Gender:</b> {{gender}} </div> 
               <div><b>Age:</b> {{age}} </div> 
               <br>
             {{/each}}
             </ul>
             </li>
           {{/each}}
           {{#if groups}}
           </ul>
           {{/if}}
          </li>  
          {{/each}}
          </ul>
