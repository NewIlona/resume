
<div class="col-12">
     <div class="row p-3 g-3 p-3 border-bottom border-4">
          <div class="col-8 p-2">
          <div class="row p-3">

{{#with formBlock}}

<div class="col-12 p-3">


    <div class="row p-2">
                <p class="fs-3 fw-bold">{{header}}</p>
                {{#with rating}}
                <p class="fs-6 fw-normal ms-1">{{title}} : {{value}}</p>
                {{/with}}
    </div>

    {{#with actionBlock}}

    <ul class="nav nav-tabs">
    {{#each tabs }}
     <li class="nav-item">
         <a class="nav-link {{#if isActive}} active {{/if}}" href="#">{{text}}</a>
     </li>
    {{/each}}
    </ul>

<br>

<div class="row border-top border-bottom border-5 py-5 my-5">
     {{#with report}}
     <div class="card card-body border-2 my-5 p-5" style="background-color:rgba(0, 140, 255, 0.048)">
        <p class="fw-medium">{{title}}</p>
        <div class="col-4">
        {{#with button}}
        <a type="button" class="btn btn-secondary" href="{{link}}" target="_blank">{{text}}</a>
        {{/with}}
        </div>
     </div>
    {{/with}}
</div>

{{/with}}
{{#with feedbackBlock}}
<div class="row p-2 py-5">

    <p class="py-3 fs-3 fw-bold">{{title}}</p>
<div class="col-7">
<div class="row justify-content-between">
   {{#each buttons}}
<div class="col-4 text-light"> 
    <div class="row g-2">
{{> buttons}}
</div>
 </div>
   {{/each}}
</div>
</div>
</div>

{{/with}}

</div>
</div>
</div>
 <div class="col-4">

<div class="row justify-content-center p-2">
    {{#with goodsInfo}}
    <div class="col-12">
<div class="row">
 {{> shopcard}}
</div>

<div class="row my-3">
<div class="card px-4 card-body ">
    {{> check}}
</div>
</div>

<div class="row my-3">
<div class="card px-4 card-body ">
    {{#with seller}}
    {{> seller}}
    {{/with}}
</div>
</div>
    </div>
    {{/with}}
</div>
</div>
{{/with}}
</div>

{{#each reviewList}}
{{> reviewitem}}
{{/each}}
</div>

    <div class="card text-bg-success">
       {{#with service}}
          <h2 class="text-center mb-4">{{title}}</h2>
          <p class="text-center mb-4">{{description}}</p>
          {{#each buttons}}
          <div class="text-center mb-8">
          <div class="col-12 mb-4">
           <a class="btn btn-primary"  type="button" href="{{link}}">{{text}}</a>    
        </div> 
        </div>
         {{/each}}
         {{/with}}
     </div>
</section>
<br>
<hr>
<br>
<section>
    <div class="row">
        {{#each footer}}
        <div class="col-2">
            <div class="row">
                <div class="nav flex-column">
                    <div class="nav-item">
                        {{#each this}}

                          <a class="nav-link" href="{{link}}">{{text}}</a>

                        {{/each}}
                         <div class="nav-item">
                          <a class="nav-link disabled">Disabled</a>
                         </div>
                    </div>
                </div>
            </div>
        </div>
        {{/each}}
    </div>


